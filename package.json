{
    "name": "mobx-context-typescript",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "mobx": "^6.3.1",
        "mobx-react": "^7.2.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "4.0.3",
        "typescript": "^4.1.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "@types/jest": "^26.0.15",
        "@types/node": "^12.0.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "prettier": "^2.3.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eslint": "eslint --fix .",
        "format": "prettier --write \"*.{css,js,json,md,ts,tsx,d.ts}\" \"./**/*.{css,js,json,md,ts,tsx,d.ts}\"",
        "lint": "yarn format && yarn eslint",
        "transpile": "yarn clean && tsc ts/*/* --outDir js",
        "header:comment": "node js/*/header_comments.js && yarn lint",
        "generated:acknowledgments": "node js/*/generated_acknowledgments.js && yarn lint",
        "clean": "rimraf js ACKNOWLEDGMENTS.md"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "files": [
        "src/index.tsx",
        "ACKNOWLEDGMENTS.md",
        "mobx/models/index.ts"
    ]
}
